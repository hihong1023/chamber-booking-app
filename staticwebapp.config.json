{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 200,
      "serve": "/index.html"
    },
    "404": {
      "statusCode": 200,
      "serve": "/index.html"
    }
  }
}
